VertexLitGeneric
{
	"$basetexture"	"freak_fortress_2\doom\doom"
	"$bumpmap" 		"freak_fortress_2\doom\doom_n"
	"$model" "1"

	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost"	"0.5"
	"$phongfresnelranges"	"[0.2 0.2 0.2]"
	
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6


	"$yellow" "0"
	"$one" "1"
	
	// Cloaking
	"$cloakPassEnabled" "1"

	"360?$color2" "[ 0.7 0.7 0.7 ]"

	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}

	}
}