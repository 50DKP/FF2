"VertexlitGeneric"
{
	"$baseTexture" "freak_fortress_2/fairyheavy/heavy_fairy_wings"
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"

	"$baseAlphaEnvMapMask" "1" 
	"$additive"	1


	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" "1"
//	"$lightwarptexture" "models/player/items/engineer/hwn_engineer_warp"	
	"$phongfresnelranges"	"[.5 .5 .5]"
	"$phongwarptexture" "models/player/pyro/pyro_lightwarp"
	"$basemapalphaphongmask" "1"

	">=DX90"
	{
	"$envmap" "env_cubemap"
	"$envmaptint" "[.2 .2 .2]"
	}




	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "15"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "4"										// Boost for ambient cube component of rim lighting


	"$blendtintbybasealpha" "1"
	"$blendtintcoloroverbase" "0"
	
	"$colortint_base" "{134 51 43}"
	"$colortint_tmp" "{0 0 0}"

	"$color2" "{251 166 187}" 	

	// Cloaking
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
  		"ItemTintColor"
  		{
   			"resultVar" "$colortint_tmp"
  		}
  		"SelectFirstIfNonZero"
 		{
   			"srcVar1" "$colortint_tmp"
   			"srcVar2" "$colortint_base"
   			"resultVar" "$color2"
  		}

	}
}



